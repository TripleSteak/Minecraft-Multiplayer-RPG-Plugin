package me.simon76800.library.entity.player;

public class PlayerCharacter {
	private Player player;
	private Races race;
	private Classes classs;
	private String name;
	
	private int level;

	public PlayerCharacter(Races race, Classes classs, String name, int level) {
		this.race = race;
		this.classs = classs;
		this.name = name;
		this.level = level;
	}
	
	public PlayerCharacter loadFromFile() {
		
	}

	public Races getRace() {
		return race;
	}

	public Classes getCharacterClass() {
		return classs;
	}
	
	public String getName() {
		return name;
	}
	
	public int getLevel() {
		return level;
	}
}
